#include <bits/stdc++.h>
using namespace std;

vector<int> runningSum(vector<int>& nums) {
    vector<int> result(nums.size());
    int sum = 0;
    for (int i = 0; i < nums.size(); i++) {
        sum += nums[i];
        result[i] = sum;
    }
    return result;
}

int main() {
    int n;
    cin >> n;
    vector<int> nums(n);

    for (int i = 0; i < n; i++) {
        cin >> nums[i];
    }

    vector<int> ans = runningSum(nums);

    for (int val : ans) {
        cout << val << " ";
    }
    cout << endl;

    return 0;
}
